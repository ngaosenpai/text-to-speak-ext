{
  "name": "La Làng",
  "description": "Extension của Hào Lê, giúp những người lười đọc :D",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts" : [
    {
      "matches" : ["<all_urls>"],
      "js" : ["content.js"]
    }
  ],
  "background" : {
    "script" : ["background.js"]
  },
  "action" : {
    "default_popup" : "popup.html"
  }
}